{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "ghcr.io/linuxserver/calibre-web"
      ],
      "allowedVersions": "<1"
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "tomsquest/docker-radicale"
      ],
      "allowedVersions": "/^[0-9]+\\.[0-9]+\\.[0-9]+(\\.[0-9]+)?$/"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["immich", "kopia"],
      "allowedVersions": "<10"
    }
  ]
}
