{
  "files.associations": {
    "*.json5": "jsonc",
    "**/ansible/**/*.yml": "ansible",
    "**/ansible/**/*.sops.yml": "yaml",
    "**/ansible/**/inventory/**/*.yml": "yaml",
    "**/terraform/**/*.tf": "terraform"
  },
"material-icon-theme.folders.associations": {
  ".taskfiles": "utils",
  "hack": "scripts"
},
"yaml.schemas": {
    "Kubernetes": "k8s/*.yaml"
  },
  "favorites.resources": [
    {
      "filePath": "k8s/clusters/hegira/flux/vars/cluster-config.yaml",
      "group": "Default"
    },
    {
      "filePath": "k8s/clusters/hegira/flux/vars/cluster-secrets.sops.yaml",
      "group": "Default"
    },
    {
      "filePath": "cluster/production/cluster-secrets.sops.yaml",
      "group": "Default"
    },
    {
      "filePath": "k8s/global/flux/vars/global-config.yaml",
      "group": "Default"
    },
    {
      "filePath": "k8s/global/flux/vars/global-secrets.sops.yaml",
      "group": "Default"
    },
    {
      "filePath": "k8s/clusters/hegira/flux/orchestration",
      "group": "Default"
    },
    {
      "filePath": "k8s/manifests/services/hajimari/helmrelease.yaml",
      "group": "Default"
    }
  ],
  "cSpell.enabled": false,
  "favorites.sortOrder": "MANUAL",
  "prettier.configPath": ".github/linters/.prettierrc.yaml",
  "prettier.ignorePath": ".github/linters/.prettierignore",
  "editor.fontFamily": "FiraCode Nerd Font Mono, 'Droid Sans Mono', 'monospace', monospace",
  "editor.fontLigatures": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs":"active",
  "editor.hover.delay": 1500,
  "explorer.autoReveal": false,
  "conventionalCommits.scopes": [
    "vscode",
    "lidarr",
    "ci",
    "trilio",
    "qbittorrent",
    "k10",
    "flux",
    "ingress-nginx",
    "authentik",
    "node-problem",
    "cluster",
    "language-tools",
    "satisfactory",
    "actions",
    "arc",
    "nzbget",
    "static",
    "blackbox",
    "hs110",
    "hass",
    "openbooks"
  ],
  "discord.enabled": true,

}
